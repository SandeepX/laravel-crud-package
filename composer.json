{
    "name": "mr.incognito/crudify",
    "description": "This package is for handling the crud for simple crud like for setup etc.",
    "type": "laravel-package",
    "license": "MIT",
    "keywords": [
        "laravel",
        "crudify",
        "crud-generator",
        "api",
        "api-crud",
        "web",
        "web-crud",
        "laravel-api",
        "package",
        "laravel-package",
        "code-generator",
        "scaffold",
        "boilerplate",
        "model",
        "controller",
        "request-validation",
        "resource",
        "migration",
        "artisan",
        "php",
        "backend",
        "developer-tool",
        "rapid-development",
        "laravel-toolkit",
        "developer-helper"
    ],
    "authors": [
        {
            "name": "Sandeep pant",
            "email": "sandeeppant024@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3|^8.4",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/filesystem": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/validation": "^10.0|^11.0||^12.0",
        "illuminate/routing": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "MrIncognito\\CrudGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MrIncognito\\CrudGenerator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MrIncognito\\CrudGenerator\\ApiCrudGeneratorServiceProvider"
            ]
        }
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "laravel/pint": "^1.20",
        "pestphp/pest": "^2.0||^3.0",
        "pestphp/pest-plugin-laravel": "^2.4",
        "orchestra/testbench": "^8.0 || ^9.0 ||^10.0",
        "rector/rector-laravel": "^2.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/pest",
        "pint": "vendor/bin/pint",
        "rector": "vendor/bin/rector"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }

}
